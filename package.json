{
	"name": "mountainvue",
	"displayName": "MountainVue",
	"publisher": "MountainVue",
	"description": "TBA",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Visualization"
	],
	"keywords": [
		"vue",
		"vs code extension",
		"component hierarchy",
		"devtools"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "mv-sidebar-view",
					"title": "MountainVue",
					"icon": "media/mv.svg"
				}
			]
		},
		"views": {
			"mv-sidebar-view": [
				{
					"type": "webview",
					"id": "mv-sidebar",
					"name": "MountainVue",
					"icon": "media/mv.svg",
					"contextualTitle": "MountainVue"
				}
			]
		},
		"commands": []
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"build": "concurrently \"cross-env NODE_ENV=development \" \"webpack\" \"npm-run-all -p build:*\"",
		"build:extension": "webpack --mode production",
		"build:sidebar": "webpack --config ./src/webviews/webpack.views.config.js",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"compile-tests": "tsc -p . --outDir out",
		"watch-tests": "tsc -p . -w --outDir out",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@babel/parser": "^7.16.2",
		"@babel/types": "^7.16.0",
		"@types/esprima": "^4.0.3",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.62.0",
		"@types/vue": "^2.0.0",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"@vscode/test-electron": "^1.6.2",
		"@vue/cli-plugin-babel": "^4.5.15",
		"@vue/cli-service": "^4.5.15",
		"@vue/compiler-sfc": "^3.2.22",
		"concurrently": "^6.3.0",
		"cross-env": "^7.0.3",
		"css-loader": "^6.5.1",
		"eslint": "^8.1.0",
		"esprima": "^4.0.1",
		"file-loader": "^6.2.0",
		"generate-source-map": "^0.0.5",
		"glob": "^7.1.7",
		"mini-css-extract-plugin": "^2.4.4",
		"mocha": "^9.1.3",
		"node-sass": "^6.0.1",
		"npm-run-all": "^4.1.5",
		"postcss-import": "^14.0.2",
		"sass": "^1.43.4",
		"sass-loader": "^12.3.0",
		"style-loader": "^3.3.1",
		"ts-loader": "^9.2.6",
		"typescript": "^4.4.4",
		"vscode-test": "^1.6.1",
		"vue": "^3.2.22",
		"vue-loader": "^16.8.3",
		"vue-loader-plugin": "^1.3.0",
		"vue-style-loader": "^4.1.3",
		"vue-template-compiler": "^2.6.14",
		"webpack": "^5.52.1",
		"webpack-cli": "^4.8.0"
	},
	"dependencies": {
		"core-js": "^3.19.1",
		"path": "^0.12.7",
		"register-service-worker": "^1.7.2",
		"vue-ast": "^0.1.1"
	}
}
